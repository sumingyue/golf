<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="imageSpecialDetail" extends="golf">
		<action name="imageSpecialDetailList" class="imageSpecialDetailAction" method="imageSpecialDetailList">
			<result name="success">jsp/imageSpecialDetail/ImageSpecialDetailList.jsp</result>
		</action>
		<action name="imageSpecialDetailAdd" class="imageSpecialDetailAction" method="imageSpecialDetailAdd">
			<result name="success">jsp/imageSpecialDetail/ImageSpecialDetailAdd.jsp</result>
		</action>
		<action name="imageSpecialDetailAddSubmit" class="imageSpecialDetailAction" method="imageSpecialDetailAddSubmit">
			<result name="success" type="redirect">imageSpecialDetailList.action</result>
		</action>
		<action name="imageSpecialDetailUpdate" class="imageSpecialDetailAction" method="imageSpecialDetailUpdate" >
			<result name="success">jsp/imageSpecialDetail/ImageSpecialDetailUpdate.jsp</result>
		</action>
		<action name="imageSpecialDetailUpdateSubmit" class="imageSpecialDetailAction" method="imageSpecialDetailUpdateSubmit">
			<result name="success" type="redirect">imageSpecialDetailList.action</result>
		</action>
		<action name="imageSpecialDetailDelete" class="imageSpecialDetailAction" method="imageSpecialDetailDelete">
			<result name="success" type="redirect">imageSpecialDetailList.action</result>
		</action>
	</package>	
</struts>
